// FOR LOOPS
// iterate through the numbers array and add the value of the numbers array to total
const numbers = [2, 3, 4, 5, 6];

// declare and intialize total to = 0
let total = 0;

for (let i = 0; i < numbers.length; i++) {
    total += numbers[i]; // adding each array element to total
};

console.log(total);


// WHILE LOOPS
const num = 0;

while (num < 10) {
    console.log(num)
    num++; // increment
}

console.log(num)

// add numbers to an empty array
const nums = []; // create a empty array
const i = 1; // create a variable i to 1

while (i < 10) {
    nums.push(i); // add the numbers to the nums array
    i++ // increment
};

console.log(nums);


// NESTED FOR LOOPS

// ACCESSING MULTI DEMENSIONAL ARRAY
function multiplyNumbers(num) {
    const item = 1; // 1 is the number we start from as we iterate over the nested array
    
    for (let i = 0; i < num.length; i++) {
      for (let j = 0; j < num[i].length; j++) { // here we check the num[i].length 
        item *= num[i][j] // multiply item by num[i][j] index
      }
    }
    // Only change code above this line
    return item;
  }
  
  
 console.log(multiplyNumbers([[1,2],[3,4],[5,6,7]]));










