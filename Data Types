// Primitive Data Types:
#1 STRING 
// typeof 'hello' = string
// typeof '123454' = string

// intergers and floating numbers are the same in JavaScript
#2 NUMBER
// typeof 1231234 = number
// typeof 1.2132 = number

// returns TRUE OR FALSE
#3 BOOLEAN
// typeof true = boolean
// typeof false = boolean

// numbers that end with n are called BigInt
#4 BigInt
// typeof 12132n = BigInt
// typeof 1n = BigInt

#5 NULL
// null represents a empty or unknown value
// typeof null = null
const something = []

#6 UNDEFINED
// undefined indicates a variable has not been assigned a value or declared
typeof undefined = undefined
const something;
